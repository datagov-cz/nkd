  ?datováSada a dcat:Dataset, dcat:DatasetSeries ;
    dcterms:title ?názevDatovéSady ;
    dcterms:description ?popisDatovéSady ;
    dcterms:publisher <https://rpp-opendata.egon.gov.cz/odrpp/zdroj/orgán-veřejné-moci/17651921> ;
    #dcterms:publisher ?OhlasujeAgendu ;
    dcatap:applicableLegislation <http://data.europa.eu/eli/reg/2022/868/oj> ;
    dcat:keyword ?MaNazevAgendy ;
    dcterms:accessRights <http://publications.europa.eu/resource/authority/access-right/RESTRICTED> .

}
WHERE {
  ?Agenda a l-sgov-sbírka-111-2009-pojem:agenda ;
    l-sgov-sbírka-111-2009-pojem:má-název-agendy ?MaNazevAgendy ;
    l-sgov-sbírka-111-2009-pojem:má-kód-agendy ?MaKodAgendy ;
    ^l-sgov-sbírka-111-2009-pojem:ohlašuje-agendu ?OhlasujeAgendu .

  OPTIONAL { ?Agenda a-sgov-104-pojem:má-datum-zániku-agendy ?MaDatumZanikuAgendy .}
  OPTIONAL { ?Agenda a-sgov-104-pojem:má-platnost-agendy-do ?MaPlatnostAgendyDo . } 

  {
    SELECT ?Agenda (GROUP_CONCAT(?radek; separator=\"\\n\") AS ?OnSUy)
    WHERE {
      {
        SELECT ?Agenda ?kódOnSÚ ?OnSÚNázev
        WHERE {
          [] a-sgov-104-pojem:sdružuje-údaje-vedené-nebo-vytvářené-v-rámci-agendy ?Agenda ;
                a-sgov-104-pojem:má-kód-objektu-nebo-subjektu-údajů ?kódOnSÚ ;
                a-sgov-104-pojem:má-název-objektu-nebo-subjektu-údajů ?OnSÚNázev .
        }
        ORDER BY ?kódOnSÚ
      }
      BIND(CONCAT(?kódOnSÚ, \": \", ?OnSÚNázev) AS ?radek)
    }
    GROUP BY ?Agenda
  }

  FILTER (LANG(?MaNazevAgendy) = \"cs\")
  FILTER (!BOUND(?MaDatumZanikuAgendy) || ?MaDatumZanikuAgendy > NOW())
  FILTER (!BOUND(?MaPlatnostAgendyDo) || ?MaPlatnostAgendyDo > NOW())

  BIND(STRLANG(CONCAT(?MaKodAgendy, \": \", ?MaNazevAgendy), \"cs\") AS ?názevDatovéSady)
  BIND(STRLANG(CONCAT(\"Datová sada obsahuje informace o subjektech a objektech práva a jejich vlastnostech v rámci agendy \", ?MaKodAgendy, \": \", ?MaNazevAgendy, \", která je reprezentována jako položka inventárního seznamu ČR.\\nSubjekty a objekty práva jsou následující:\\n\", ?OnSUy), \"cs\") AS ?popisDatovéSady)
  BIND(IRI(CONCAT(\"https://data.gov.cz/zdroj/datové-sady/inventární-seznam/agendy/\", ?MaKodAgendy )) AS ?datováSada)
}""";
    <http://plugins.linkedpipes.com/ontology/t-sparqlConstruct#queryControl> <http://plugins.linkedpipes.com/resource/configuration/None> .
