<https://oha02.dia.gov.cz/etl/resources/pipelines/1693834852539-0001/component/0a60-aade/configuration> {
  <https://oha02.dia.gov.cz/etl/resources/pipelines/1693834852539-0001/component/0a60-aade/configuration>
    a <http://plugins.linkedpipes.com/ontology/t-sparqlConstruct#Configuration>;
    <http://plugins.linkedpipes.com/ontology/t-sparqlConstruct#query> """PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX l-sgov-sbírka-111-2009-pojem: <https://slovník.gov.cz/legislativní/sbírka/111/2009/pojem/> 
PREFIX a-sgov-104-pojem: <https://slovník.gov.cz/agendový/104/pojem/>
PREFIX dcatap: <http://data.europa.eu/r5r/>

CONSTRUCT { 
  ?datováSada a dcat:Dataset ;
    dcterms:title ?názevDatovéSady ;
    dcterms:description ?popisDatovéSady ;
    dcterms:publisher <https://rpp-opendata.egon.gov.cz/odrpp/zdroj/orgán-veřejné-moci/17651921> ;
    #dcterms:publisher ?OhlasujeAgendu ;
    dcatap:applicableLegislation <http://data.europa.eu/eli/reg/2022/868/oj> ;
    dcat:keyword ?MaNazevAgendy, ?OnSÚNázev;
    dcterms:accessRights <http://publications.europa.eu/resource/authority/access-right/RESTRICTED> ;
    dcat:inSeries ?datováSérieAgendy .
	
  ?datováSérieAgendy dcat:seriesMember ?datováSada .
}
WHERE {
  ?Agenda a l-sgov-sbírka-111-2009-pojem:agenda ;
    l-sgov-sbírka-111-2009-pojem:má-název-agendy ?MaNazevAgendy ;
    l-sgov-sbírka-111-2009-pojem:má-kód-agendy ?MaKodAgendy ;
    ^l-sgov-sbírka-111-2009-pojem:ohlašuje-agendu ?OhlasujeAgendu .

  OPTIONAL { ?Agenda a-sgov-104-pojem:má-datum-zániku-agendy ?MaDatumZanikuAgendy .}
  OPTIONAL { ?Agenda a-sgov-104-pojem:má-platnost-agendy-do ?MaPlatnostAgendyDo . } 

  ?OnSU a-sgov-104-pojem:sdružuje-údaje-vedené-nebo-vytvářené-v-rámci-agendy ?Agenda ;
        a-sgov-104-pojem:má-kód-objektu-nebo-subjektu-údajů ?kódOnSÚ ;
        a-sgov-104-pojem:má-název-objektu-nebo-subjektu-údajů ?OnSÚNázev ;
        a-sgov-104-pojem:má-popis-objektu-nebo-subjektu-údajů ?OnSÚPopis .

  {
    SELECT ?OnSU (GROUP_CONCAT(?radek; separator=\"\\n\") AS ?udaje)
    WHERE {
      {
        SELECT ?OnSU ?kodUdaje ?nazevUdaje
        WHERE {
          ?OnSU a-sgov-104-pojem:má-údaj ?udaj .
          ?udaj a-sgov-104-pojem:má-kód-údaje ?kodUdaje ;
                a-sgov-104-pojem:má-název-údaje ?nazevUdaje .
        }
        ORDER BY ?kodUdaje
      }
      BIND(CONCAT(?kodUdaje, \": \", ?nazevUdaje) AS ?radek)
    }
    GROUP BY ?OnSU
  }

  FILTER (LANG(?MaNazevAgendy) = \"cs\")
  FILTER (!BOUND(?MaDatumZanikuAgendy) || ?MaDatumZanikuAgendy > NOW())
  FILTER (!BOUND(?MaPlatnostAgendyDo) || ?MaPlatnostAgendyDo > NOW())

  BIND(STRLANG(CONCAT(?kódOnSÚ, \": \", ?OnSÚNázev), \"cs\") AS ?názevDatovéSady)
  BIND(STRLANG(CONCAT(\"Datová sada obsahuje informace o subjektu/objektu práva a jeho vlastnostech, který je veden v agendě \", ?MaKodAgendy, \": \", ?MaNazevAgendy, \".\\n Základní popis: \", ?OnSÚPopis, \"\\nEvidované vlastnosti/údaje:\\n\", ?udaje), \"cs\") AS ?popisDatovéSady)
  BIND(IRI(CONCAT(\"https://data.gov.cz/zdroj/datové-sady/inventární-seznam/agendy/\", ?MaKodAgendy )) AS ?datováSérieAgendy)
  BIND(IRI(CONCAT(STR(?datováSérieAgendy), \"/subjekty-a-objekty-údajů/\", ?kódOnSÚ )) AS ?datováSada)

}""";
    <http://plugins.linkedpipes.com/ontology/t-sparqlConstruct#queryControl> <http://plugins.linkedpipes.com/resource/configuration/None> .
}

