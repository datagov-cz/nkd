<https://oha02.dia.gov.cz/etl/resources/pipelines/1736242591316-0001/component/3543-9132/configuration> {
  <https://oha02.dia.gov.cz/etl/resources/pipelines/1736242591316-0001/component/3543-9132/configuration>
    a <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#Configuration>;
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#fileName> "historie-datových-sad.csv";
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#fileNameControl> <http://plugins.linkedpipes.com/resource/configuration/None>;
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#query> """PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX dcat: <http://www.w3.org/ns/dcat#>

PREFIX : <http://localhost/stat/>

PREFIX nkod: <https://data.gov.cz/zdroj/katalog/NKOD>

SELECT ?datová_sada ?název_datové_sady ?poskytovatel ?název_poskytovatele ?naposledy_spatřeno
WHERE { 
  nkod: dcterms:modified ?datum_harvestace .
  
  ?datová_sada a dcat:Dataset ;
  	dcterms:title ?název_datové_sady ;
  	dcterms:publisher ?poskytovatel .
  
  ?poskytovatel foaf:name ?název_poskytovatele .
  
  OPTIONAL { ?datová_sada dcterms:valid ?naposledy_spatřeno_z_dat . }
  
  FILTER(langMatches(LANG(?název_datové_sady), \"cs\"))
  FILTER(langMatches(LANG(?název_poskytovatele), \"cs\"))
  
  # Dataset v dnešním NKOD - datum harvestace.
  # Dataset v historii a ne v dnešním NKOD - naposledy spatřeno.
  BIND(NOT EXISTS { ?datová_sada a :DnešníDataset } AS ?dnes_nepřítomen)
  BIND(IF(?dnes_nepřítomen, ?naposledy_spatřeno_z_dat, ?datum_harvestace) AS ?naposledy_spatřeno)
}
    
    
    
    
    """;
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#queryControl> <http://plugins.linkedpipes.com/resource/configuration/None> .
}

