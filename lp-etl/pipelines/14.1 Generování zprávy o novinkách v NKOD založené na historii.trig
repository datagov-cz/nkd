<https://oha02.dia.gov.cz/etl/resources/pipelines/1736242591316-0001/component/69c3-8424/configuration> {
  <https://oha02.dia.gov.cz/etl/resources/pipelines/1736242591316-0001/component/69c3-8424/configuration>
    a <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#Configuration>;
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#fileName> "ještě-neviděné-datové-sady.csv";
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#fileNameControl> <http://plugins.linkedpipes.com/resource/configuration/None>;
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#query> """PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX dcat: <http://www.w3.org/ns/dcat#>

PREFIX : <http://localhost/stat/>

PREFIX nkod: <https://data.gov.cz/zdroj/katalog/NKOD>

SELECT ?datová_sada ?název_datové_sady ?poskytovatel ?název_poskytovatele
WHERE { 
  ?datová_sada a dcat:Dataset, :DnešníDataset ;
  	dcterms:title ?název_datové_sady ;
  	dcterms:publisher ?poskytovatel .
  
  ?poskytovatel foaf:name ?název_poskytovatele .
  
  FILTER(langMatches(LANG(?název_datové_sady), \"cs\"))
  FILTER(langMatches(LANG(?název_poskytovatele), \"cs\"))
  
  FILTER NOT EXISTS { ?datová_sada a :JižViděnýDataset }
}
    
    
    
    
    """;
    <http://plugins.linkedpipes.com/ontology/t-sparqlSelect#queryControl> <http://plugins.linkedpipes.com/resource/configuration/None> .
}

