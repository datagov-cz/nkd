  ?řádek ř:datov%C3%A1_sada ?IRI_datové_sady ;
         ř:poskytovatel ?IRI_poskytovatele ;
         ř:n%C3%A1zev_datov%C3%A9_sady ?název_datové_sady_cs ;
         ř:n%C3%A1zev_poskytovatele ?název_poskytovatele_cs ;
         ř:naposledy_spat%C5%99eno ?naposledy_viděno_str.
  
  BIND(IRI(?IRI_datové_sady) AS ?datová_sada)
  BIND(IRI(?IRI_poskytovatele) AS ?poskytovatel)
  BIND(STRLANG(?název_datové_sady_cs, \"cs\") AS ?název_datové_sady)
  BIND(STRLANG(?název_poskytovatele_cs, \"cs\") AS ?název_poskytovatele)
  BIND(STRDT(?naposledy_viděno_str, xsd:date) AS ?naposledy_viděno)
}""";
